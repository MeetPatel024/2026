   "source": [
    "nums = [1,3,5,8,9,14,15,19,20,21]\n",
    "n = len(nums)\n",
    "target = 16\n",
    "low = 0\n",
    "high = n-1\n",
    "lb = n\n",
    "while low<=high:\n",
    "    mid = (low+high)//2\n",
    "    if nums[mid]>=target:\n",
    "        lb = mid\n",
    "        high = mid-1\n",
    "    else:\n",
    "        low = mid+1\n",
    "print(lb)\n",
    "\n"
   ]
